<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nextep.pelmel"
    android:versionCode="12"
    android:versionName="2.6.3" >

    <uses-sdk
        android:minSdkVersion="12"
        android:targetSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_GPS" />

    <!-- Google MAPS permissions -->
    <permission
        android:name="com.nextep.pelmel.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" >
    </permission>

    <uses-permission android:name="com.nextep.pelmel.permission.MAPS_RECEIVE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <!-- END OF Google MAPS permissions -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <!-- PUSH NOTIFICATIONS PERMISSIONS -->
    <permission android:name="com.nextep.pelmel.permission.C2D_MESSAGE" android:protectionLevel="signature" />
    <uses-permission android:name="com.nextep.pelmel.permission.C2D_MESSAGE" />
    <!--<uses-permission android:name="android.permission.INTERNET" />-->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".PelMelApplication"
        android:allowBackup="true"
        android:icon="@drawable/pelmel_icon"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >

        <receiver android:name="com.nextep.pelmel.push.GCMReceiver"
                  android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />
                <category android:name="com.nextep.pelmel" />
            </intent-filter>
        </receiver>
        <service
            android:name="com.nextep.pelmel.push.MyGcmListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name="com.nextep.pelmel.push.MyInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID"/>
            </intent-filter>
        </service>
        <activity
            android:name=".activities.ListPlacesActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".activities.OverviewActivity" />

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDZrq-ZiDdyYRBcoNjLrul-J11vo8oqC0M" />
        <!-- PROD -->
            <!--android:value="AIzaSyDZrq-ZiDdyYRBcoNjLrul-J11vo8oqC0M" />-->
        <!-- DEBUG -->
        <!--android:value="AIzaSyCUrzVh3F9ttywWpMdtRfRUrkpLKCCGUIQ" />-->
        <!-- DON'T KNOW, NOT WORKING -->
        <!--android:value="AIzaSyCrHWO2iZlLYbCM3U9UrCOYNQ6kiMmxWTQ" />-->

        <activity android:name=".activities.MyProfileActivity" />
        <activity android:name=".activities.PhotosManagerActivity" />
        <activity android:name=".activities.LoginActivity" android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".activities.ListEventsActivity" />
        <activity android:name=".activities.TextActivity" />
        <activity android:name=".activities.GalleryActivity" />
        <activity android:name=".activities.MainActionBarActivity" />
        <activity
            android:name=".activities.MainActivity" android:label="">
